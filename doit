python esgen.py --datasource cgh_100000/gorilla.7.100000  --chromosome 7 --species gorilla --index cgh_100000 --processing-config data/config/config.test --idprefix gorilla_7 --outfile cgh_100000/gorilla.7.100000.es_bulk
python mk50.py cgh_100000/gorilla.7.100000.es_bulk 1000
python mkload.py cgh_100000/gorilla.7.100000.es_bulk > cgh_100000/load_gorilla_7
chmod +x cgh_100000/load_gorilla_7
./load_gorilla_7
