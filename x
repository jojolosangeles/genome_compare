+    "def sp_to_y(val):\n",
+    "    if val == 'human':\n",
+    "        return 0\n",
+    "    elif val == 'chimp':\n",
+    "        return 200\n",
+    "    else:\n",
+    "        return 400\n",
+    "df['y'] = [ sp_to_y(val) for val in df['sp']]\n",
+    "df['y2'] = [ sp_to_y(val) for val in df['msp']]"
+   ]
